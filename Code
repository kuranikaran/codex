def is_palindrome(num):
    return str(num) == str(num)[::-1]

def solve (N):
    # Write your code here
    if N <=0:
        return ""
    num = int ('1' + '0' * (N -2) + '1')

    while True:
        if num %7 == 0 and is_palindrome(num):
            return num
        num += 10
   

T = int(input())
for _ in range(T):
    N = int(input())
    out_ = solve(N)
    print (out_)
